<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <type-mapping>
  	<sql-type jdbc-type="NVARCHAR" hibernate-type="string"/>
  </type-mapping>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="Dispatchment"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="EmDepartment"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="EmRole"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="Employee"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="Member"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="OStatus"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="Order"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="OrderItem"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="PaymentMethod"/>
  <table-filter match-catalog="ERPWork" match-schema="dbo" match-name="PostalCode"/>
</hibernate-reverse-engineering>
